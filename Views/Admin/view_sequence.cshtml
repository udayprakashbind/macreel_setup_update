@model List<macreel_setup.Models.admin.manage_sequence>

@{
    ViewBag.Title = "view_sequence";
    Layout = "~/Views/Shared/admin_mst.cshtml";
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>View Sequence List</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Sequence List</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>


    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">

                            <h3 class="card-title float-right">Sequence  List </h3>
                        </div>
                        <div class="card-body">
                            <table id="example" class="table table-bordered table-hover justify-content-center text-center">
                                <thead>
                                    <tr class="bg-primary">
                                        <th>Sr.No.</th>
                                        <th>Branch</th>
                                        <th>Quotation Prefix No</th>
                                        <th>QuotationNo</th>
                                        <th>Sale Book PrefixNo</th>
                                        <th>PI Prefix No</th>
                                        <th>PINo</th>
                                        <th>PO Prefix No</th>
                                        <th>PO No</th>
                                        <th>Invoice Prefix No</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        int i = 1;
                                        foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>@i</td>
                                                <td>@item.Branch</td>
                                                <td>@item.QuotationPrefixNo</td>
                                                <td>@item.QuotationNo</td>
                                                <td>@item.SaleBookPrefixNo</td>
                                                <td>@item.PIPrefixNo</td>
                                                <td>@item.PINo</td>
                                                <td>@item.POPrefixNo</td>
                                                <td>@item.PONo</td>
                                                <td>@item.InvoicePrefixNo</td>
                                                <td><a href="/admin/add_sequence?id=@(item.id)" class="btn btn-success" style="width:50px"><i class="fas fa-edit"></i></a> || <a href="/admin/delet_sequence?id=@(item.id)" class="btn btn-danger " style="width:50px"> <i class="fas fa-solid fa-trash"></i></a></td>
                                            </tr>
                                            i++;
                                        }

                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

            </div>


        </div>


        <!-- /.container-fluid -->
    </section>

    </div>